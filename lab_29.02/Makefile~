all: obj obj/storage_class.o obj/main.o 
	g++ obj/main.o obj/storage_class.o

obj:
	mkdir obj

obj/storage_class.o: src/storage_class.cpp inc/storage_class.h
	g++ -c src/storage_class.cpp -o obj/storage_class.o

obj/main.o: src/main.cpp inc/storage_class.h
	g++ -c src/main.cpp -o obj/main.o

clean:
	rm obj/*.o a.out

